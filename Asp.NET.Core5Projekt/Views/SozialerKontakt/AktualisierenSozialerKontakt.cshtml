@model NSchicht.Core.Konkret.SozialerKontakt
@{
    ViewData["Title"] = "AktualisierenSozialerKontakt";
    Layout = "~/Views/Shared/_NeuerAdministratorLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Soziale Medien Bearbeiten Site</h4>
                  <form class="pt-3" method="post" >
                      @Html.HiddenFor(x=>x.SozialerKontaktID)
                     <div class="form-group">
                      <label for="exampleInputName1">Titlle</label>
                      <input type="text" asp-for="Name" class="form-control" id="exampleInputName1" placeholder="Name">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputName1">Url</label>
                      <input type="text" asp-for="Url" class="form-control" id="exampleInputName1" placeholder="Url">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputName1">Ikon</label>
                      <input type="text" asp-for="Ikon" class="form-control" id="exampleInputName1" placeholder="Ikon">
                    </div>
                    <button type="submit" class="btn btn-outline-primary mr-2">Bearbeiten</button>
                    </form>
                </div>
              </div>
            </div>

