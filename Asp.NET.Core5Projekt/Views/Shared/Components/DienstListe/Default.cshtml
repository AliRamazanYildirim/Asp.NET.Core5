
@using NSchicht.Core.Konkret
@model List<Dienst>
<div class="section px-3 px-lg-4 pt-5" id="dienste">
  <div class="container-narrow">
    <div class="text-center mb-5">
      <h2 class="marker marker-center">Meine Dienste</h2>
    </div>
    <div class="text-center">
        <p class="mx-auto mb-3" style="max-width:600px"> Ich biete Dienstleistungen an, die für jede Website oder App geeignet sind. Ich kann zeitnahe Ergebnisse für Echtzeit-Schemata schnell maximieren.</p>
    </div>
    <div class="row py-3">
        @foreach (var item in Model)
       {
           <div class="col-md-3 text-center" data-aos="fade-up" data-aos-delay="100"><img class="mb-2" src="@item.BildUrl" width="96" height="96" alt="web design"/>
        <div class="h5">@item.Title</div>
      </div>
       }
    </div>
  </div>
</div>